{
    "name": "dandysi/laravel-batch-upload",
    "description": "A framework to help manage/automate/process data uploads (such as CSV), using your business logic",
    "keywords": ["php", "laravel", "upload", "batch"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Simon Adams"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dandysi\\Laravel\\BatchUpload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dandysi\\Laravel\\BatchUpload\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.37",
        "illuminate/console": "^10.37"
    },
    "require-dev": {
        "orchestra/testbench": "^8.18",
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.42",
        "phpstan/phpstan": "^1.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dandysi\\Laravel\\BatchUpload\\BatchUploadServiceProvider"
            ]
        }
    }
}
